    <element id="CarePlan.subject">
        <path value="CarePlan.subject" />
        <mustSupport value="true" />
    </element>
    <element id="CarePlan.period">
        <path value="CarePlan.period" />
        <min value="1" />
        <mustSupport value="true" />
    </element>
    <element id="CarePlan.careTeam">
        <path value="CarePlan.careTeam" />
        <min value="1" />
        <mustSupport value="true" />
    </element>
</differential>
</StructureDefinition>
