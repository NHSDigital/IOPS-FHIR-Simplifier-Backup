  <name value="UKCoreMedicationDispense" />
  <title value="UK Core MedicationDispense" />
  <status value="active" />
  <date value="2023-12-12" />
  <publisher value="HL7 UK" />
  <contact>
    <name value="HL7 UK" />
    <telecom>
      <system value="email" />
