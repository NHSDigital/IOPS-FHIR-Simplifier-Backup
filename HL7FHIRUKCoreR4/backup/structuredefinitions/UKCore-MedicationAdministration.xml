  <version value="2.3.0" />
  <name value="UKCoreMedicationAdministration" />
  <title value="UK Core MedicationAdministration" />
  <status value="active" />
  <date value="2023-12-12" />
  <publisher value="HL7 UK" />
  <contact>
    <name value="HL7 UK" />
    <telecom>
      <system value="email" />
      <value value="ukcore@hl7.org.uk" />
