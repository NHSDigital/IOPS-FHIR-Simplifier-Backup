<StructureDefinition xmlns="http://hl7.org/fhir">
  <id value="England-Organization" />
  <url value="https://fhir.nhs.uk/StructureDefinition/England-Organization" />
  <version value="1.0.0" />
  <name value="EnglandOrganization" />
  <title value="England Organization" />
  <status value="draft" />
  <date value="2023-08-18" />
  <publisher value="NHS England" />
  <contact>
    <name value="NHS England" />
    <telecom>
      <system value="email" />
      <value value="interoperabilityteam@nhs.net" />
      <use value="work" />
      <rank value="1" />
    </telecom>
  </contact>
  <description value="This profile defines the NHS England constraints and extensions on the FHIR UK Core resource [UK Core Organization](https://simplifier.net/hl7fhirukcorer4/ukcore-organization)." />
  <purpose value="This profile allows exchange of a formally or informally recognised grouping of people or organisations formed for the purpose of achieving some form of collective action. Includes companies, institutions, corporations, departments, community groups, healthcare practice groups, etc." />
